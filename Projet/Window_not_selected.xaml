<Window x:Class="Projet.Window_not_selected"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Error" Height="150" Width="375" ResizeMode="NoResize" Icon="favicon.ico" ShowInTaskbar="False" WindowStartupLocation="CenterScreen" KeyDown="Window_KeyDown">
    
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.5*"/>
            <ColumnDefinition Width="0.5*"/>
        </Grid.ColumnDefinitions>
        
        <Grid.RowDefinitions>
            <RowDefinition Height="0.7*"/>
            <RowDefinition Height="0.3*"/>
        </Grid.RowDefinitions>
        
        <Label Content="Emot not selected" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Button Content="OK" Grid.Column="0" Grid.Row="1" Height="20" Width="70" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Button_Click"/>
        
    </Grid>
</Window>
